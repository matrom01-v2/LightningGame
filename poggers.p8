--player
player = {
 landed = true,
 x = 0,
 y = 48,
 dy = 0,
 acc = 0.16,
 
 jump = function(player)
  player.dy = -2.7
  player.landed = false
 end,
 
 die = function(player)
  sfx(0)
  //run()
	 player:change_level()
 end,
 
 change_level = function(player)
		if (level == 1) then
		 player.x = 0
		 player.y = 48
		end
		if (level == 2) then
		 player.x = 0
		 player.y = 96
		end
	end,
	 
 move = function(player)
  player.x += player.acc
 end,
 
 yset = function(player)
  player.y += player.dy
  player.dy += 0.2
 end,
 
 dyset = function(player, set)
  player.dy = set
 end
}
